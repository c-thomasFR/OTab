{
   "background":
   {
      "scripts":
      [
         "./assets/js/jquery-3.3.1.min.js"
      ]
   },
   "chrome_url_overrides" :
   {
      "newtab": "index.html"
   },
   "description": "One tab to find and rule all newtab. It allows you to run in your new tab chrome extensions that override the newtab",
   "manifest_version": 2,
   "name": "OTab",
   "permissions":
   [
      "activeTab",
      "management",
      "storage",
      "tabs"
   ],
   "icons": {
      "16": "./assets/img/icon16.png",
      "32": "./assets/img/icon32.png",
      "48": "./assets/img/icon48.png",
      "128": "./assets/img/icon128.png"
    },
   "version": "1.0"
}
